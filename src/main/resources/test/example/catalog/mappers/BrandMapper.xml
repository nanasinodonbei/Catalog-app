<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.example.catalog.mappers.BrandMapper">
  <select id="selectAll" resultType="test.example.catalog.beans.Brand">
    SELECT
       brand_id,
       brand_name
    FROM
       brand
    ORDER BY
       brand_id;
 </select>
</mapper>